{
	"name": "adobly",
	"version": "2.0.0",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --noEmit",
		"build:esm": "swc ./src --out-dir ./dist",
		"build": "yarn workspace @adobly/framework run build && yarn build:clean && yarn build:check && yarn build:esm",
		"start": "env-cmd node --enable-source-maps dist/index.js",
		"dev": "yarn build && yarn deploy:commands && yarn start",
		"deploy:commands": "env-cmd node --enable-source-maps dist/deploy.js"
	},
	"type": "module",
	"directories": {
		"lib": "src"
	},
	"keywords": [],
	"author": "WoozyStudio <woozystudiocontact@gmail.com>",
	"license": "MIT",
	"private": true,
	"dependencies": {
		"@adobly/framework": "workspace:^",
		"discord.js": "14.21.0",
		"env-cmd": "10.1.0",
		"path": "0.12.7"
	},
	"devDependencies": {
		"@swc/cli": "0.1.63",
		"@swc/core": "1.3.100",
		"@types/node": "22.14.1",
		"tsc-watch": "6.2.1",
		"tslib": "2.8.1",
		"typescript": "5.8.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
